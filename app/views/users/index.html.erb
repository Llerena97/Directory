<% @users.each do |user| %>
  <div style="border: black solid;">
  <h2><%= link_to user.first_name, user %> <%= link_to user.last_name, user %></h2>
  <h4><%= user.company %></h4>
  <h5>Created at: <%= time_ago_in_words(user.created_at) %></h5>
  <h5>Updated at: <%= time_ago_in_words(user.updated_at) %></h5>
  </div>
<%end%>